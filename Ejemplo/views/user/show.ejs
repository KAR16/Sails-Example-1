<div class="container">
	<h2> Users Information </h2>
	<ul>
		<li><%= user.name %></li>
		<li><%= user.lastname %></li>
		<li><%= user.username %></li>
		<li><%= user.email %></li>
	</ul>

	<form action="/user/edit/<%= user.id %>" method="POST" accept-charset="utf-8">
		<button class="btn btn-primary" type="submit">Next</button>
	</form>
</div>